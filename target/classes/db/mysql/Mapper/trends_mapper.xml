<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"
        >

<mapper namespace="trends">




    <select id="queryById" parameterType="TrendsEntity" resultMap="queryMap">
        SELECT id,send_user_id,content FROM trends ;
    </select>

    <resultMap id="queryMap" type="TrendsEntity">
        <id column="id"  property="id"/>
        <result column="send_user_id" property="send_user_id"/>
        <result column="content" property="content"/>
    </resultMap>

</mapper>